{
  "env": {
    "es2021": true,
    "browser": true
  },
  "extends": [
    "standard-with-typescript",
    "plugin:react/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "typescript-eslint/no-misused-promises": "off"
  }
}
